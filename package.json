{
  "version": "0.5.6",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "server": "nodemon --watch src --exec 'node -r esbuild-register' src/server.ts",
    "build": "tsdx build --target=node",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@labdigital/commercetools-mock",
  "author": "Michael van Tellingen",
  "module": "dist/commercetools--mock.esm.js",
  "size-limit": [
    {
      "path": "dist/commercetools--mock.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/commercetools--mock.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@types/basic-auth": "^1.1.3",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.11",
    "@types/morgan": "^1.9.2",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "esbuild-register": "^2.4.0",
    "got": "^11.8.2",
    "husky": "^6.0.0",
    "nodemon": "^2.0.7",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@commercetools/platform-sdk": "^1.10.0",
    "basic-auth": "^2.0.1",
    "deep-equal": "^2.0.5",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "nock": "^13.0.11",
    "perplex": "^0.11.0",
    "pratt": "^0.7.0",
    "supertest": "^6.1.3"
  }
}

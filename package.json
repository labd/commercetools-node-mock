{
  "name": "@labdigital/commercetools-mock",
  "author": "Michael van Tellingen",
  "version": "1.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/commercetools--mock.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    }
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8.1.1"
  },
  "packageManager": "pnpm@8.1.1",
  "scripts": {
    "start": "tsup src --watch src/server.ts",
    "build": "tsup",
    "build:server": "esbuild src/server.ts --bundle --outfile=dist/server.js --platform=node",
    "publish:ci": "pnpm build && pnpm changeset publish",
    "check": "eslint src && tsc",
    "test": "jest test --coverage",
    "lint": "eslint src"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "@types/lodash": "^4.14.182",
    "basic-auth": "^2.0.1",
    "body-parser": "^1.20.0",
    "deep-equal": "^2.0.5",
    "express": "^4.17.2",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nock": "^13.2.1",
    "perplex": "^0.11.0",
    "pratt": "^0.7.0",
    "supertest": "^6.1.6",
    "type-fest": "^3.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-typescript": "^7.18.6",
    "@changesets/cli": "^2.26.0",
    "@changesets/changelog-github": "^0.4.8",
    "@commercetools/platform-sdk": "4.0.0",
    "@labdigital/eslint-config-node": "0.0.5",
    "@types/basic-auth": "^1.1.3",
    "@types/body-parser": "^1.19.2",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.29",
    "@types/jest": "^28.1.6",
    "@types/morgan": "^1.9.3",
    "@types/node": "*",
    "@types/qs": "^6.9.7",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "esbuild": "^0.14.50",
    "esbuild-register": "^3.3.1",
    "eslint": "^8.20.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "got": "^11.8.3",
    "husky": "^7.0.4",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "timekeeper": "^2.2.0",
    "ts-node": "^10.4.0",
    "tsc": "^2.0.4",
    "tslib": "^2.3.1",
    "tsup": "^6.2.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@commercetools/platform-sdk": "^2.4.1"
  }
}

{
	"name": "@labdigital/commercetools-mock",
	"version": "2.50.1",
	"license": "MIT",
	"author": "Michael van Tellingen",
	"type": "module",
	"exports": {
		".": "./dist/index.js"
	},
	"main": "dist/index.js",
	"module": "dist/index.js",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"build": "tsdown",
		"build:server": "esbuild src/server.ts --bundle --outfile=dist/server.js --platform=node",
		"check": "biome check && tsc",
		"format": "biome check --fix",
		"lint": "biome check",
		"publish:ci": "pnpm build && pnpm changeset publish",
		"publish:version": "pnpm changeset version && pnpm format",
		"start": "tsdown src/server.ts --watch --onSuccess 'node dist/server'",
		"test": "vitest run",
		"test:ci": "vitest run --coverage"
	},
	"dependencies": {
		"basic-auth": "2.0.1",
		"body-parser": "2.2.0",
		"decimal.js": "10.5.0",
		"express": "5.1.0",
		"light-my-request": "6.6.0",
		"morgan": "1.10.0",
		"msw": "2.7.3",
		"uuid": "11.1.0",
		"zod": "3.24.2",
		"zod-validation-error": "3.4.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@types/express": "^5.0.1",
		"@changesets/changelog-github": "0.5.1",
		"@changesets/cli": "2.28.1",
		"@commercetools/platform-sdk": "8.8.0",
		"@types/basic-auth": "1.1.8",
		"@types/body-parser": "1.19.5",
		"@types/express-serve-static-core": "^5.0.6",
		"@types/morgan": "1.9.9",
		"@types/node": "20.16.14",
		"@types/qs": "6.9.11",
		"@types/supertest": "6.0.2",
		"@types/uuid": "9.0.8",
		"@vitest/coverage-v8": "3.1.1",
		"esbuild": "0.25.2",
		"fishery": "2.2.3",
		"supertest": "7.1.0",
		"timekeeper": "2.3.1",
		"tsdown": "^0.9.9",
		"typescript": "5.8.3",
		"vitest": "3.1.1"
	},
	"packageManager": "pnpm@10.8.0",
	"engines": {
		"node": ">=18",
		"pnpm": ">=9.0.2"
	},
	"publishConfig": {
		"access": "public"
	}
}

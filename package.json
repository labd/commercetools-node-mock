{
	"name": "@labdigital/commercetools-mock",
	"author": "Michael van Tellingen",
	"version": "2.16.1",
	"license": "MIT",
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"type": "module",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"engines": {
		"node": ">=18",
		"pnpm": ">=8.6.5"
	},
	"packageManager": "pnpm@8.6.5",
	"scripts": {
		"start": "tsup src/server.ts --watch --onSuccess 'node dist/server.js'",
		"build": "tsup",
		"build:server": "esbuild src/server.ts --bundle --outfile=dist/server.js --platform=node",
		"publish:ci": "pnpm build && pnpm changeset publish",
		"test": "vitest run",
		"test:ci": "vitest run --coverage",
		"check": "pnpm lint && tsc",
		"format": "eslint src --fix ; prettier --write .",
		"lint": "eslint src && prettier --check ."
	},
	"publishConfig": {
		"access": "public"
	},
	"prettier": {
		"printWidth": 80,
		"semi": false,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"dependencies": {
		"basic-auth": "^2.0.1",
		"body-parser": "^1.20.2",
		"deep-equal": "^2.2.3",
		"express": "^4.18.2",
		"light-my-request": "^5.11.0",
		"lodash.isequal": "^4.5.0",
		"morgan": "^1.10.0",
		"msw": "^2.1.2",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@changesets/changelog-github": "^0.5.0",
		"@changesets/cli": "^2.27.1",
		"@commercetools/platform-sdk": "7.2.0-alpha.2",
		"@types/basic-auth": "^1.1.7",
		"@types/body-parser": "^1.19.5",
		"@types/deep-equal": "^1.0.4",
		"@types/express": "^4.17.21",
		"@types/express-serve-static-core": "^4.17.41",
		"@types/lodash.isequal": "^4.5.8",
		"@types/morgan": "^1.9.9",
		"@types/node": "^20.11.5",
		"@types/qs": "^6.9.11",
		"@types/supertest": "^6.0.2",
		"@types/uuid": "^9.0.7",
		"@typescript-eslint/eslint-plugin": "^6.19.0",
		"@typescript-eslint/parser": "^6.19.0",
		"@vitest/coverage-v8": "^1.2.1",
		"esbuild": "^0.19.11",
		"eslint": "^8.56.0",
		"eslint-plugin-unused-imports": "^3.0.0",
		"got": "^14.0.0",
		"husky": "^8.0.3",
		"prettier": "^3.2.4",
		"supertest": "^6.3.4",
		"timekeeper": "^2.3.1",
		"ts-node": "^10.9.2",
		"tslib": "^2.6.2",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3",
		"vitest": "^1.2.1"
	}
}

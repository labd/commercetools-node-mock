{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "additionalProperties": false,
  "title": "ShoppingList",
  "type": "object",
  "properties": {
    "textLineItems": {
      "type": "array",
      "items": {"$ref": "TextLineItem.schema.json"}
    },
    "anonymousId": {"type": "string"},
    "lastModifiedAt": {
      "type": "string",
      "format": "date-time"
    },
    "lastModifiedBy": {"$ref": "LastModifiedBy.schema.json"},
    "custom": {"$ref": "CustomFields.schema.json"},
    "deleteDaysAfterLastModification": {"type": "number"},
    "description": {"$ref": "LocalizedString.schema.json"},
    "store": {"$ref": "StoreKeyReference.schema.json"},
    "version": {"type": "number"},
    "lineItems": {
      "type": "array",
      "items": {"$ref": "ShoppingListLineItem.schema.json"}
    },
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {"$ref": "CreatedBy.schema.json"},
    "name": {"$ref": "LocalizedString.schema.json"},
    "id": {"type": "string"},
    "key": {"type": "string"},
    "slug": {"$ref": "LocalizedString.schema.json"},
    "customer": {"$ref": "CustomerReference.schema.json"}
  },
  "required": [
    "id",
    "version",
    "createdAt",
    "lastModifiedAt",
    "name"
  ],
  "$id": "https://api.commercetools.com/ShoppingList.schema.json"
}

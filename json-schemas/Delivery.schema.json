{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "additionalProperties": false,
  "title": "Delivery",
  "type": "object",
  "properties": {
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "id": {"type": "string"},
    "address": {"$ref": "Address.schema.json"},
    "items": {
      "type": "array",
      "items": {"$ref": "DeliveryItem.schema.json"}
    },
    "parcels": {
      "type": "array",
      "items": {"$ref": "Parcel.schema.json"}
    },
    "custom": {"$ref": "CustomFields.schema.json"}
  },
  "required": [
    "id",
    "createdAt",
    "items",
    "parcels"
  ],
  "$id": "https://api.commercetools.com/Delivery.schema.json"
}

{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "additionalProperties": false,
  "title": "TaxRateDraft",
  "type": "object",
  "properties": {
    "name": {"type": "string"},
    "country": {"type": "string"},
    "amount": {"type": "number"},
    "state": {"type": "string"},
    "includedInPrice": {"type": "boolean"},
    "subRates": {
      "type": "array",
      "items": {"$ref": "SubRate.schema.json"}
    }
  },
  "required": [
    "name",
    "includedInPrice",
    "country"
  ],
  "$id": "https://api.commercetools.com/TaxRateDraft.schema.json"
}

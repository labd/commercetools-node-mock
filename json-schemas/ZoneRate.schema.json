{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "description": "Defines shipping rates in different currencies for a specific [Zone](ctp:api:type:Zone).",
  "additionalProperties": false,
  "title": "ZoneRate",
  "type": "object",
  "properties": {
    "zone": {"$ref": "ZoneReference.schema.json"},
    "shippingRates": {
      "type": "array",
      "items": {"$ref": "ShippingRate.schema.json"}
    }
  },
  "required": [
    "zone",
    "shippingRates"
  ],
  "$id": "https://api.commercetools.com/ZoneRate.schema.json"
}

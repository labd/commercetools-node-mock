{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "additionalProperties": false,
  "title": "StoreDraft",
  "type": "object",
  "properties": {
    "supplyChannels": {
      "type": "array",
      "items": {"$ref": "ChannelResourceIdentifier.schema.json"}
    },
    "languages": {
      "type": "array",
      "items": {"type": "string"}
    },
    "distributionChannels": {
      "type": "array",
      "items": {"$ref": "ChannelResourceIdentifier.schema.json"}
    },
    "custom": {"$ref": "CustomFieldsDraft.schema.json"},
    "name": {"$ref": "LocalizedString.schema.json"},
    "key": {"type": "string"},
    "productSelections": {
      "type": "array",
      "items": {"$ref": "ProductSelectionSettingDraft.schema.json"}
    }
  },
  "required": ["key"],
  "$id": "https://api.commercetools.com/StoreDraft.schema.json"
}

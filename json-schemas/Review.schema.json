{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "additionalProperties": false,
  "title": "Review",
  "type": "object",
  "properties": {
    "lastModifiedAt": {
      "type": "string",
      "format": "date-time"
    },
    "uniquenessValue": {"type": "string"},
    "lastModifiedBy": {"$ref": "LastModifiedBy.schema.json"},
    "custom": {"$ref": "CustomFields.schema.json"},
    "rating": {"type": "number"},
    "locale": {"type": "string"},
    "title": {"type": "string"},
    "version": {"type": "number"},
    "target": {"type": [
      "number",
      "string",
      "boolean",
      "object",
      "array",
      "null"
    ]},
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "includedInStatistics": {"type": "boolean"},
    "createdBy": {"$ref": "CreatedBy.schema.json"},
    "authorName": {"type": "string"},
    "id": {"type": "string"},
    "text": {"type": "string"},
    "state": {"$ref": "StateReference.schema.json"},
    "key": {"type": "string"},
    "customer": {"$ref": "CustomerReference.schema.json"}
  },
  "required": [
    "id",
    "version",
    "createdAt",
    "lastModifiedAt",
    "includedInStatistics"
  ],
  "$id": "https://api.commercetools.com/Review.schema.json"
}

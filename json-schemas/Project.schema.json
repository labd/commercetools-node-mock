{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "additionalProperties": false,
  "title": "Project",
  "type": "object",
  "properties": {
    "externalOAuth": {"$ref": "ExternalOAuth.schema.json"},
    "shoppingLists": {"$ref": "ShoppingListsConfiguration.schema.json"},
    "languages": {
      "type": "array",
      "items": {"type": "string"}
    },
    "searchIndexing": {"$ref": "SearchIndexingConfiguration.schema.json"},
    "countries": {
      "type": "array",
      "items": {"type": "string"}
    },
    "version": {"type": "number"},
    "trialUntil": {"type": "string"},
    "shippingRateInputType": {"$ref": "ShippingRateInputType.schema.json"},
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "carts": {"$ref": "CartsConfiguration.schema.json"},
    "name": {"type": "string"},
    "messages": {"$ref": "MessagesConfiguration.schema.json"},
    "key": {"type": "string"},
    "currencies": {
      "type": "array",
      "items": {"type": "string"}
    }
  },
  "required": [
    "version",
    "key",
    "name",
    "countries",
    "currencies",
    "languages",
    "createdAt",
    "messages",
    "carts"
  ],
  "$id": "https://api.commercetools.com/Project.schema.json"
}

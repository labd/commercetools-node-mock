{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "additionalProperties": false,
  "title": "Extension",
  "type": "object",
  "properties": {
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "lastModifiedAt": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {"$ref": "CreatedBy.schema.json"},
    "timeoutInMs": {"type": "number"},
    "lastModifiedBy": {"$ref": "LastModifiedBy.schema.json"},
    "destination": {"$ref": "ExtensionDestination.schema.json"},
    "id": {"type": "string"},
    "triggers": {
      "type": "array",
      "items": {"$ref": "ExtensionTrigger.schema.json"}
    },
    "version": {"type": "number"},
    "key": {"type": "string"}
  },
  "required": [
    "id",
    "version",
    "createdAt",
    "lastModifiedAt",
    "destination",
    "triggers"
  ],
  "$id": "https://api.commercetools.com/Extension.schema.json"
}

{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "additionalProperties": false,
  "title": "Parcel",
  "type": "object",
  "properties": {
    "createdAt": {
      "type": "string",
      "format": "date-time"
    },
    "id": {"type": "string"},
    "items": {
      "type": "array",
      "items": {"$ref": "DeliveryItem.schema.json"}
    },
    "measurements": {"$ref": "ParcelMeasurements.schema.json"},
    "trackingData": {"$ref": "TrackingData.schema.json"},
    "custom": {"$ref": "CustomFields.schema.json"}
  },
  "required": [
    "id",
    "createdAt"
  ],
  "$id": "https://api.commercetools.com/Parcel.schema.json"
}

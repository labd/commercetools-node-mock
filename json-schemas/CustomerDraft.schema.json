{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "additionalProperties": false,
  "title": "CustomerDraft",
  "type": "object",
  "properties": {
    "lastName": {"type": "string"},
    "addresses": {
      "type": "array",
      "items": {"$ref": "BaseAddress.schema.json"}
    },
    "companyName": {"type": "string"},
    "vatId": {"type": "string"},
    "billingAddresses": {
      "type": "array",
      "items": {"type": "number"}
    },
    "title": {"type": "string"},
    "locale": {"type": "string"},
    "isEmailVerified": {"type": "boolean"},
    "password": {"type": "string"},
    "authenticationMode": {"$ref": "AuthenticationModeEnum.schema.json"},
    "email": {"type": "string"},
    "key": {"type": "string"},
    "anonymousId": {"type": "string"},
    "defaultShippingAddress": {"type": "number"},
    "customerGroup": {"$ref": "CustomerGroupResourceIdentifier.schema.json"},
    "stores": {
      "type": "array",
      "items": {"$ref": "StoreResourceIdentifier.schema.json"}
    },
    "custom": {"$ref": "CustomFieldsDraft.schema.json"},
    "externalId": {"type": "string"},
    "dateOfBirth": {"type": [
      "number",
      "string",
      "boolean",
      "object",
      "array",
      "null"
    ]},
    "customerNumber": {"type": "string"},
    "firstName": {"type": "string"},
    "anonymousCart": {"$ref": "CartResourceIdentifier.schema.json"},
    "middleName": {"type": "string"},
    "salutation": {"type": "string"},
    "anonymousCartId": {"type": "string"},
    "defaultBillingAddress": {"type": "number"},
    "shippingAddresses": {
      "type": "array",
      "items": {"type": "number"}
    }
  },
  "required": ["email"],
  "$id": "https://api.commercetools.com/CustomerDraft.schema.json"
}

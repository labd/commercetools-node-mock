    },
    "id": {"type": "string"},
    "version": {"type": "number"},
    "key": {"type": "string"},
    "status": {"$ref": "SubscriptionHealthStatusEnum.schema.json"}
  },
  "required": [
    "id",
    "version",
    "createdAt",
    "lastModifiedAt",
    "changes",
    "destination",
    "messages",
    "format",
    "status"

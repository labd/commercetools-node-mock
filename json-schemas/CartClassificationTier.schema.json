{
  "$schema": "https://json-schema.org/draft/2020-12/schema#",
  "description": "Used when the ShippingRate maps to an abstract Cart categorization expressed by strings (for example, `Light`, `Medium`, or `Heavy`).",
  "additionalProperties": false,
  "title": "CartClassificationTier",
  "type": "object",
  "properties": {
    "type": {"enum": ["CartClassification"]},
    "value": {"type": "string"},
    "isMatching": {"type": "boolean"},
    "price": {"$ref": "Money.schema.json"}
  },
  "required": [
    "type",
    "value",
    "price"
  ],
  "$id": "https://api.commercetools.com/CartClassificationTier.schema.json"
}
